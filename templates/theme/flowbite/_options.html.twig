{% if results is empty %}
    <div class="autocomplete-empty px-4 py-2 text-sm text-gray-500">
        No results
    </div>
{% else %}
    <ul class="autocomplete-list py-1 text-sm text-gray-700" role="listbox" aria-label="Autocomplete results">
        {% for item in results %}
            <li class="autocomplete-option cursor-pointer px-4 py-2 hover:bg-gray-100"
                role="option"
                tabindex="-1"
                data-autocomplete-option-value="{{ item.id }}"
                data-autocomplete-option-label="{{ item.label }}"
                    {% if item.meta is defined %}
                        data-autocomplete-option-meta="{{ item.meta|json_encode|e('html_attr') }}"
                    {% endif %}
            >
                {{ item.label }}
            </li>
        {% endfor %}
    </ul>
{% endif %}
