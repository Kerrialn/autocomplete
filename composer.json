{
  "name": "kerrialnewham/autocomplete",
  "description" : "",
  "type": "symfony-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Kerrialnewham\\Autocomplete\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "test-app/src/"
    }
  },
  "authors": [
    {
      "name": "kez",
